@using CarInsurance.Models
@model List<Insured>


<div class="row row-no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Entries</h5>
        </div>
        <div class="card-body">
            @foreach (var insuree in Model)
                {
          <div class="row">
            <div class="col">
              @insuree.FirstName
            </div>
            <div class="col">
              @insuree.LastName
            </div>
            <div class="col">
              @insuree.EmailAddress
            </div>
            <div class="col">
              @insuree.Quote
            </div>
          </div>
                }

          
        </div>
    </div>
</div>